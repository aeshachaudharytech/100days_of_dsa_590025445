#include <stdio.h>

int main() {
    int n;

    // Input size
    scanf("%d", &n);

    int matrix[n][n];

    // Read matrix
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            scanf("%d", &matrix[i][j]);
        }
    }

    int is_identity = 1;   // 1 = true, 0 = false

    // Check identity condition
    for (int i = 0; i < n && is_identity; i++) {
        for (int j = 0; j < n; j++) {
            if (i == j) {
                if (matrix[i][j] != 1) {
                    is_identity = 0;
                    break;
                }
            } else {
                if (matrix[i][j] != 0) {
                    is_identity = 0;
                    break;
                }
            }
        }
    }

    // Output
    if (is_identity)
        printf("Identity Matrix\n");
    else
        printf("Not an Identity Matrix\n");

    return 0;
}
