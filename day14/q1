# Input size
n = int(input())

# Read matrix
matrix = []
for i in range(n):
    row = list(map(int, input().split()))
    matrix.append(row)

is_identity = True

# Check identity condition
for i in range(n):
    for j in range(n):
        if i == j:
            if matrix[i][j] != 1:
                is_identity = False
                break
        else:
            if matrix[i][j] != 0:
                is_identity = False
                break
    if not is_identity:
        break

# Output
if is_identity:
    print("Identity Matrix")
else:
    print("Not an Identity Matrix")
