#include <stdio.h>
#include <string.h>

int main() {
    char s[1000];
    
    // Input
    scanf("%s", s);

    int left = 0;
    int right = strlen(s) - 1;
    int is_palindrome = 1;   // 1 = true, 0 = false

    // Two-pointer comparison
    while (left < right) {
        if (s[left] != s[right]) {
            is_palindrome = 0;
            break;
        }
        left++;
        right--;
    }

    // Output
    if (is_palindrome)
        printf("YES\n");
    else
        printf("NO\n");

    return 0;
}

