#include <stdio.h>
#include <string.h>

// Recursive function to print string in reverse
void mirror(char s[], int index) {
    if (index < 0)      // Base case
        return;

    printf("%c", s[index]);   // Print current character
    mirror(s, index - 1);     // Recursive call
}

int main() {
    char code[100];

    // Input
    scanf("%s", code);

    int length = strlen(code);

    // Output (mirror format)
    mirror(code, length - 1);

    return 0;
}
