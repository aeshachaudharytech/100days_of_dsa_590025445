#include <stdio.h>

int main() {
    int n, k;
    int arr[100];
    int comparisons = 0;

    // Read array size
    scanf("%d", &n);

    // Read array elements
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    // Read key
    scanf("%d", &k);

    // Linear search
    for (int i = 0; i < n; i++) {
        comparisons++;
        if (arr[i] == k) {
            printf("Found at index %d\n", i);
            printf("Comparisons = %d\n", comparisons);
            return 0;
        }
    }

    // If key not found
    printf("Not Found\n");
    printf("Comparisons = %d\n", comparisons);

    return 0;
}
